{
  "name": "eslint-config-uphold-react",
  "version": "2.5.0",
  "description": "Uphold-flavored React ESLint config",
  "keywords": [
    "config",
    "eslint",
    "lint",
    "shared"
  ],
  "homepage": "https://github.com/uphold/eslint-config-uphold-react#readme",
  "bugs": {
    "url": "https://github.com/uphold/eslint-config-uphold-react/issues"
  },
  "license": "MIT",
  "author": "Uphold",
  "main": "./src/index.js",
  "repository": "uphold/eslint-config-uphold-react",
  "files": [
    "src"
  ],
  "scripts": {
    "changelog": "github-changelog-generator --future-release=v$npm_config_future_release > CHANGELOG.md",
    "lint": "eslint --config src/index.js src/index.js test/rules.test.js",
    "test": "node --test test/rules.test.js",
    "test-update-snapshots": "node --test --test-update-snapshots test/rules.test.js",
    "version": "npm run changelog --future-release=$npm_package_version && git add -A CHANGELOG.md"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.23.0",
    "eslint-config-uphold": "^6.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sort-class-members": "1.20.0",
    "globals": "^16.0.0"
  },
  "devDependencies": {
    "@uphold/github-changelog-generator": "3.4.0",
    "eslint": "~8.57.0",
    "pre-commit": "1.2.2",
    "prettier": "^3.5.3",
    "react": "~18.2.0",
    "release-it": "^17.10.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "peerDependencies": {
    "eslint": "~9.23.0",
    "prettier": ">=3.0.0"
  },
  "engines": {
    "node": "^22.14.0"
  }
}
